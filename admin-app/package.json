{
  "name": "freedomlabs-admin-app",
  "version": "2.0.16",
  "buildNumber": 38,
  "description": "Freedom Labs - Admin Desktop App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "prebuild": "node ../increment-build.js admin-app",
    "prebuild:win": "node ../increment-build.js admin-app",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux"
  },
  "keywords": [
    "electron",
    "freedomlabs",
    "admin"
  ],
  "author": "Freedom Labs",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "firebase": "^12.4.0",
    "electron-updater": "^6.6.2"
  },
  "build": {
    "appId": "com.freedomlabs.admin.v2",
    "productName": "Freedom Labs Admin v2",
    "win": {
      "target": "nsis",
      "artifactName": "Freedom Labs Admin v2 Setup.exe"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": {
      "provider": "generic",
      "url": "https://drive.google.com/uc?export=download&id=1j_cwLaJkfgoimdzt7n7ecGvMfoCpW9_9"
    }
  }
}
