{
  "name": "freedomlabs-assistant-app",
  "version": "1.0.25",
  "buildNumber": 34,
  "description": "Freedom Labs - Assistant Desktop App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "prebuild": "node ../increment-build.js assistant-app",
    "prebuild:win": "node ../increment-build.js assistant-app",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux"
  },
  "keywords": [
    "electron",
    "freedomlabs",
    "assistant"
  ],
  "author": "Freedom Labs",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "active-win": "^8.2.1",
    "electron-updater": "^6.6.2",
    "firebase": "^12.4.0",
    "screenshot-desktop": "^1.15.3"
  },
  "build": {
    "appId": "com.freedomlabs.assistant",
    "productName": "Freedom Labs Assistant",
    "win": {
      "target": "nsis",
      "artifactName": "Freedom Labs Assistant Setup.exe"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": {
      "provider": "generic",
      "url": "https://drive.google.com/uc?export=download&id=1DWH-pq1rADzaAHnjeRgSzoAbFKk9wOXS"
    }
  }
}
